{
  "name": "FearlessSalaryNegotiation",
  "version": "1.0.0",
  "description": "Fearless Salary Negotiation website based on Tailwind with Jekyll",
  "main": "gulpfile.babel.js",
  "devDependencies": {
    "autoprefixer": "10.2.5",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "browser-sync": "^2.27.10",
    "browser-sync-client": "^2.27.10",
    "cross-env": "7.0.3",
    "cross-spawn": "7.0.3",
    "cssnano": "4.1.10",
    "gulp": "4.0.2",
    "gulp-postcss": "9.0.0",
    "postcss": "8.2.8",
    "postcss-import": "14.0.0",
    "tailwindcss": "2.0.4"
  },
  "scripts": {
    "build:production": "cross-env NODE_ENV=production gulp",
    "build:dev": "cross-env NODE_ENV=development gulp",
    "build": "npm run build:production",
    "dev": "cross-env NODE_ENV=development gulp serve",
    "start": "npm run dev",
    "serve:jekyll": "bundle exec jekyll serve --baseurl \"\" --incremental",
    "assets:watch": "cross-env NODE_ENV=development gulp processStyles && cross-env NODE_ENV=development gulp"
  },
  "repository": {},
  "author": "Taylor Bryant",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
